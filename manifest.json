{
  "content_scripts":[
    {
      "js":[
        "js/jquery.min.js",
        "js/plugins.js",
        "js/Chart.min.js",
        "better-envato.js"
      ],
      "css":[
        "css/envato-styles.css"
      ],
      "matches":[
        "*://*.themeforest.net/*",
        "*://*.codecanyon.net/*",
        "*://*.videohive.net/*",
        "*://*.audiojungle.net/*",
        "*://*.graphicriver.net/*",
        "*://*.photodune.net/*",
        "*://*.3docean.net/*",
        "*://*.activeden.net/*",
        "*://*.envato.com/*"
      ]
    }
  ],
  "browser_action":{
    "name":"Better Envato"
  },
  "background":{
    "page":"background.html"
  },
  "description":"Make Envato better by adding some cool features",
  "icons":{
    "128":"img/128.png",
    "16":"img/16.png",
    "48":"img/48.png"
  },
  "manifest_version":2,
  "name":"Better Envato",
  "short_name":"Better Envato",
  "options_page":"options.html",
  "permissions":[
    "storage",
    "webRequest",
    "notifications",
    "background"
  ],
  "update_url":"http://clients2.google.com/service/update2/crx",
  "version":"1.2.5"
}